<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/resultdisplay_linearlayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="center_horizontal" >
    <ImageView
        android:id="@+id/image_Res"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="top"
        android:adjustViewBounds="true"
        android:src="@drawable/guiji" />
    <TextView
        android:id="@+id/rstTexxt"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/hello_world" />

    <Button
        android:id="@+id/result_btn"
        android:layout_width="90dp"
        android:layout_height="40dp"
        android:layout_marginRight="20dp"
        android:layout_alignParentRight="true"
        android:text="@string/ok"

        android:textColor="#ffffff"
        android:textSize="18sp"
        android:onClick="returntomain"
        />
</LinearLayout><!-- From: file:/Users/JiateLi/Desktop/java-werun-430/Application/src/main/res/layout/ui_resultdisplay.xml -->